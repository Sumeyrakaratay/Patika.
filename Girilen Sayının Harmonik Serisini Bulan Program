import java.util.Scanner;

public class fonksiyonlar {

	public static void main(String[] args) {
		
		
		Scanner inp = new Scanner(System.in);
		
		System.out.print("N Sayısını Giriniz : ");
		int n = inp.nextInt();
		double result = 0.0;
		
		for (int i = 1; i <= n; i++) {
			
			result = result + (1.0/i);
			
		}
		System.out.print("Sonuç : "+ result);
	  }	
}
