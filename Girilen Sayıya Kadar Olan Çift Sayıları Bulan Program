import java.util.Scanner;

public class fonksiyonlar {

	public static void main(String[] args) {
		
		Scanner inp = new Scanner(System.in);

		int k;
		
		System.out.println("SayÄ± Giriniz: ");
		k = inp.nextInt();
		
		for (int i = 0; i<=k; i++) {
			
			if (i%2 == 0) {
				
				System.out.print(i + ", ");
		  }
		}
	 }
 }
